{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Embedding Siyabasa Docs",
  "colors": {
    "primary": "#a444f2",
    "light": "#a405e8",
    "dark": "#641287"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Welcome",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "essentials/reusable-snippets"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "API documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoint examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Remeinium",
          "href": "https://remeinium.com/",
          "icon": "home"
        },
        {
          "anchor": "HuggingFace",
          "href": "https://huggingface.com/spaces/Embedding_Siyabasa",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@remeinium.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "API Console",
      "href": "https://remeinium.mintlify.app/api-reference/introduction"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/remeinium",
      "github": "https://github.com/remeinium",
      "linkedin": "https://linkedin.com/company/remeinium"
    }
  }
}
